<xsd:schema xmlns="http://com.oocl.schema.common.event" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:common="http://com.oocl.schema.common" targetNamespace="http://com.oocl.schema.common.event" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:import namespace="http://com.oocl.schema.common" schemaLocation="../CommonRestrictions_V1.xsd"/>
	<xsd:element name="GSPPort_EVT_V1">
		<xsd:annotation>
			<xsd:documentation>This generates domain state change event for GSP Port data change (insert, update, delete)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="GSP_PortUUID" type="common:ID_Integer20_V1">
					<xsd:annotation>
						<xsd:documentation>This is the GSP Port UUID which is equivalent to GSP_PORT_RVW.GSP_PORT_UUID</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
