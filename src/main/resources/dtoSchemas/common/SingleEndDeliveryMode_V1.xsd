<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="http://com.oocl.schema.common" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:common="http://com.oocl.schema.common" targetNamespace="http://com.oocl.schema.common" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:simpleType name="SingleEndDeliveryMode_V1">
		<xsd:annotation>
			<xsd:documentation>Represents the mode of delivery on either the origin or destination side.  Can be CY or Door for Container Yard and Customer Door Location</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CY"/>
			<xsd:enumeration value="Door"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>