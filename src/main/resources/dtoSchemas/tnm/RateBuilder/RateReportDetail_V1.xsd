    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RateId" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="RateType" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="FormulaGroupId" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="SchrgOverrideGroup" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="OriginId" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="DestinationId" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="Rates" type="RateReportLevel_V1" nillable="true" minOccurs="0" maxOccurs="unbounded">
      </xsd:element>
      <xsd:element name="Currency" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="Mode" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="CargoNature" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="SpecialEquipment" type="xsd:string" nillable="true" minOccurs="0">
