      <xsd:element name="EffStartDate" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="EffEndDate" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="Notes" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="InclusiveSurcharges" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
      <xsd:element name="RatePC" type="xsd:string" nillable="true" minOccurs="0">
      </xsd:element>
    </xsd:sequence>
