<xsd:schema xmlns="http://com.oocl.schema.tnm.ratebuilder" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tnm.ratebuilder="http://com.oocl.schema.tnm.ratebuilder" targetNamespace="http://com.oocl.schema.tnm.ratebuilder" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:include schemaLocation="GuidelineRateSearchInfo_V1.xsd"/>
	<xsd:complexType name="GuidelineRateSearch_V1">
